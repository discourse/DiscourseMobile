    "invariant": "^2.2",
    "jetifier": "^1.6.3",
    "lodash": "^4.17.21",
    "moment": "^2.22",
    "prop-types": "^15.6.0",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-background-fetch": "^4.0.2",
