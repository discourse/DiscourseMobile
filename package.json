{
  "name": "Discourse",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clean-ios": "cd ios && xcodebuild -scheme Discourse -destination 'platform=iOS Simulator,name=iPhone 6' clean",
    "prettier": "prettier --write \"js/**/*.js\"",
    "lint": "eslint .",
    "test": "jest",
    "build-rn-webview": "cd node_modules/react-native-webview/ && yarn install && yarn tsc && rm -rf node_modules/react-native-webview/node_modules/",
    "postinstall": "npx jetify"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/push-notification-ios": "^1.0.3",
    "@react-native-community/segmented-control": "^2.0.0",
    "immutable": "^4.0.0-rc.12",
    "invariant": "^2.2",
    "jetifier": "^1.6.3",
    "lodash": "^4.17",
    "moment": "^2.22",
    "prop-types": "^15.6.0",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-appearance": "^0.3.1",
    "react-native-background-fetch": "^3.0.2",
    "react-native-device-info": "^3.1.4",
    "react-native-draggable-dynamic-flatlist": "^1.0.4",
    "react-native-firebase": "^5.5.7",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-immutable-list-view": "^2.0.0",
    "react-native-key-pair": "git+https://github.com/samsaffron/react-native-key-pair",
    "react-native-progress": "^4.0.3",
    "react-native-reanimated": "^1.9.0",
    "react-native-safari-view": "git+https://github.com/samsaffron/react-native-safari-view.git",
    "react-native-safari-web-auth": "1.0.4",
    "react-native-screens": "^2.7.0",
    "react-native-swipe-list-view": "^2.1.2",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "10.1.1",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "eslint": "^7.2.0",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^1.14",
    "react-test-renderer": "16.11.0"
  }
